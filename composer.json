{
    "name": "destinmedia/user-management-lite",
    "type": "project",
    "description": "A full-feature user authentication / login system embedded into a simple but powerful MVC framework structure",
    "keywords": ["login", "auth", "user", "authentication", "mvc", "membership"],
    "homepage": "https://github.com/destinmedia/user-management-lite",
    "license": "MIT",
    "autoload": {
        "psr-4": { "": ["application/core/", "application/model/"] }
    },
    "require": {
        "mailgun/mailgun-php": "^3.5",
        "symfony/http-client": "^5.4",
        "nyholm/psr7": "^1.4",
        "phpmailer/phpmailer": "~6.0",
        "gregwar/captcha": "~1.1",
        "phpunit/phpunit": "4.8.*|5.7.*",
        "google/apiclient": "^2.0",
        "vlucas/phpdotenv": "^5.4",
        "mirazmac/dotenvwriter": "^0.1.0",
        "ifsnop/mysqldump-php": "^2.9",
        "stripe/stripe-php": "^7.113",
        "league/omnipay": "^3.2",
        "omnipay/stripe": "^3.1"
    },
    "config": {
      "platform": {
        "php": "7.4.27"
      }
    }
}
